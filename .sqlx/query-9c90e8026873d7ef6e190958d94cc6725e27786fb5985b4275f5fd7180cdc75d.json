{
  "db_name": "PostgreSQL",
  "query": "\n        SELECT r.product, thumbnail, rating, title, content, products.name AS product_name\n        FROM reviews r\n        JOIN products ON products.id = r.product\n        JOIN ratings ON ratings.customer = $1 AND ratings.product = r.product\n        WHERE r.customer = $1\n        LIMIT $2\n        OFFSET $3\n        ",
